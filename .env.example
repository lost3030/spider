# 环境变量配置示例
# 复制此文件为 .env 并填入真实值

# OSS 配置
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET=your_bucket_name
OSS_REGION=cn-hangzhou

# 千问 AI 配置
QIANWEN_API_KEY=your_qianwen_api_key
QIANWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QIANWEN_MODEL=qwen-vl-plus

# 飞书 Webhook
TWITTER_FEISHU_WEBHOOK=your_feishu_webhook_url

# Twitter 配置
TWITTER_USER=elonmusk
TWITTER_HEADLESS=true
